config META_BT
	bool "Bluetooth"

	default m if META_OAK || META_SOLANA

	select NET
	select BT
	select BT_HCIBTUSB if USB

config META_BTATH3K
	tristate "ATH3K support"

	default m if META_OAK || META_SOLANA

	select NET
	select BT
	select USB
	select USB_SUPPORT
	select BT_HCIBTUSB
	select BT_ATH3K
